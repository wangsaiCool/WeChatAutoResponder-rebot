#!/usr/bin/env python3.5

from wxpy import *

from chatterbot import ChatBot

from chatterbot.trainers import ChatterBotCorpusTrainer

from chatterbot.trainers import ListTrainer



my_bot = ChatBot("Training demo")

my_bot.set_trainer(ListTrainer)

trainfile = "/home/wangsai/PycharmProjects/myRobot/conventions_train_list.txt"

file = open()


# 交互信息

print("请输入群名字：")

targetGroup = input()

# print("聊天人的名字：")

# targetGroup = input()

# 建议打开只读，防止修改数据库

chatbot = ChatBot("deepThought", read_only=True)  # 用于回复消息的机器人

chatbot.set_trainer(ChatterBotCorpusTrainer)

chatbot.train("chatterbot.corpus.chinese")  # 使用该库的中文语料库





bot = Bot(cache_path=True)  # 用于接入微信的机器人

groups = bot.groups("What")  # 进行测试的群



# for friend in bot_1.friends():

#     if (targetPerson_1 in str(friend)):

#         charP1 = friend

#         print("--Found--", str(friend))

#         break

# charP1.send("你好，我是人工智障笨笨。")



# @bot_1.register(charP1)

# @bot_1.register(charP2)

# def reply_my_friend_1(msg):

#     print(msg)

#     return chatbot_1.get_response(msg.text).text  # 使用机器人进行自动回复





for group in groups:

    if (targetGroup in str(group)):

        print("群名字：", group)

        print("群成员有：", list(group))

        print("请问这是你需要的群聊吗？是请输入 Y，否则，请输入 N")

        myGroup = group

        instr = input()

        if (instr is "Y"):

            break

        else:

            continue



myGroup.send("大家好，我是人工智障")





@bot.register(myGroup)

def reply_my_friend(msg):

    print(msg)

    return chatbot.get_response(msg.text).text  # 使用机器人进行自动回复





# 堵塞线程，并进入 Python 命令行

embed()


